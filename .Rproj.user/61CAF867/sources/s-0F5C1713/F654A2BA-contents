                   ######
                   # Tablas variable-año de indicadores Sectoriales y Complementarios
                   
                   # Indicadores Sectoriales 
                   
                   output$tabla_sec11 <- function(){
                     sec11 %>% 
                       kable() %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(1:10 , bold=F, color = "black", background = "white") %>% 
                       column_spec(11:15, bold=T, color = "black", background = 'rgb(152,152,154)') %>% 
                       row_spec(0, bold = T, color = "white", background = 'rgb(18,160,154)') 
                   }
                   
                   output$tabla_sec12 <- function(){
                     sec12 %>% 
                       kable() %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(1:10 , bold=F, color = "black", background = "white") %>% 
                       column_spec(11:15, bold=T, color = "black", background = 'rgb(152,152,154)') %>% 
                       row_spec(0, bold = T, color = "white", background = 'rgb(18,160,154)') 
                   }
                   
                   output$tabla_sec13 <- function(){
                     sec13 %>% 
                       kable() %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(1:10 , bold=F, color = "black", background = "white") %>% 
                       column_spec(11:15, bold=T, color = "black", background = 'rgb(152,152,154)') %>% 
                       row_spec(0, bold = T, color = "white", background = 'rgb(18,160,154)') 
                   }
                   
                   output$tabla_sec21 <- function(){
                     sec21 %>% 
                       kable() %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(1:10 , bold=F, color = "black", background = "white") %>% 
                       column_spec(11:15, bold=T, color = "black", background = 'rgb(152,152,154)') %>% 
                       row_spec(0, bold = T, color = "white", background = 'rgb(39,92,79)') 
                   }
                   
                   output$tabla_sec22 <- function(){
                     sec22 %>% 
                       kable() %>% 
