                                                                                 h4(strong("4.4.2 Otorgar acciones de capacitación a las personas microempresarias en zonas de marginación
                                                                              y/o alta violencia descritas en los Lineamientos de Operación del programa Microcréditos para el
                                                                              Bienestar.
                                                                              "), align = "center")
                                                                        )
                                                                      ))
                                               ))
                                      
                                      
                         )
                       ),
                       
                       
                       ##############             
                       
                       # Configuración de Tablero Plan de Reactivación Económica
                       tabItem(
                         tabName = "reactivacion",
                         h3(strong("Tablero de Control para el Plan de Reactivación Económica")),
                         hr(),
                         navlistPanel(widths = c(2,10),
                                      tabPanel("General", 
                                               #"Ventana General del Dashboard",
                                               #img(src = "tablero_general.png", height = 500, width = 900, align = "center")
                                               echarts4rOutput("avanceeje"),
                                               div( echarts4rOutput("avanceur", width = 500, height = 600), align = "center")
                                               
                                      ),
                                      tabPanel("Eje 1",  fluidPage(
                                        
                                        echarts4rOutput("directrizeje1"),
                                        echarts4rOutput("avanceeje1"),
                                        div( echarts4rOutput("donaeje1"), align = "center")
                                        
                                      )  ), 
                                      tabPanel("Eje 2",   
                                               fluidPage(
                                                 
                                                 echarts4rOutput("directrizeje2"),
                                                 echarts4rOutput("avanceeje2"),
                                                 div( echarts4rOutput("donaeje2"), align = "center")
                                                 
                                               ) 
                                               
                                      ), 
                                      tabPanel("Eje 3",  
                                               fluidPage(
                                                 
                                                 echarts4rOutput("directrizeje3"),
                                                 echarts4rOutput("avanceeje3"),
                                                 div( echarts4rOutput("donaeje3"), align = "center")
                                                 
                                               ) 
                                               
                                      ), 
                                      tabPanel("Eje 4",
                                               
                                               fluidPage(
                                                 
                                                 echarts4rOutput("directrizeje4"),
                                                 echarts4rOutput("avanceeje4"),
                                                 div( echarts4rOutput("donaeje4"), align = "center")
                                                 
                                               ) 
                                               
                                      ) 
                         )
                       ),
                       
                       #######
                       # Planeación operativa
                       tabItem(
                         tabName = "operativa",
                         h3(strong("Tablero de Control Planeación Operativa"), align = "center"),
                         hr(),
                         br(),
                         # Insertamos imagen pero con HTML
                         HTML('<center><img src="construccion.png" width="600" height="400"></center>')
                       ),
                       
                       #########
                       
                       # Configuración Acerca De
                       tabItem(
                         tabName = "acerca",
                         navbarPage("", position = "fixed-bottom",
                                    inverse = TRUE,collapsible = TRUE,
                                    tabPanel("Acerca de ",
                                             h1(strong("ACERCA DE"), align = "center"), 
                                             hr(),
                                             br(),
                                             h4("El Sistema de Monitoreo Institucional es una herramienta 
                  desarrollada por la Unidad de Prospectiva, Planeación y 
                  Evaluación de la Secretaría de Economía, y pone a disposición 
                  de los usuarios la información sobre los avances en el 
                  seguimiento del Programa Sectorial de Economía, así como el 
                  cumplimiento de las funciones más importantes de las Áreas 
                  Sustantivas de la SE.", align = "center"),
                                             br(),
                                             h4("El sistema está diseñado para brindar al usuario, de una
                  manera amigable, la consulta de la información, su exportación 
                  en diferentes formatos, así como su visualización en gráficas.", align = "center"),
                                             br(),
                                             h2(strong("Objetivo del Sistema de Monitoreo Institucional"), align = "center"),
                                             br(),
                                             h4("Integrar mediciones que permitan monitorear el quehacer 
                  del sector economía desde la planeación nacional hasta la 
                  planeación de cada de las Áreas Sustantivas.", align = "center")
                                    ),
                                    tabPanel("Términos de uso", "Contenido de términos"),
                                    tabPanel("Licencia Código Abierto", "Contenido de licencia"),
                                    tabPanel("Ayuda", "Contenido Ayuda"),
                                    tabPanel("Preguntas frecuentes", "Contenido de preguntas"),
                                    tabPanel("Contacto", "Contenido de contacto")
                         )
                       )
                       #########
                       # Paréntesis finales
                       
                     )
                   )
                 )
                 
)



                 
                 
                 
server <-  function(input, output) {
                   
                   output$avanceeje <- renderEcharts4r({
                     readxl::read_excel("pruebas/tablas_dashboard.xlsx", sheet = "avanceXeje") |>
                       mutate(color = c("#eb8034", "#2b4b4d", "#294763", "#592834") ) |> 
                       e_charts(Eje) |> 
                       e_bar(Valor) |> 
                       e_add("itemStyle", color) |> 
                       e_legend(FALSE) |> 
                       e_title("Avance promedio por Eje %", left = "center") |> 
                       e_tooltip(
                         formatter = e_tooltip_item_formatter("percent")
                       )
                     
                     
                   })
                   output$avanceur <- renderEcharts4r({
                     readxl::read_excel("pruebas/tablas_dashboard.xlsx", sheet = "Hoja2") |>
                       arrange(Promedio) |> 
                       e_charts(UR) |> 
                       e_legend(FALSE) |>
                       e_bar( Promedio) |> 
                       e_tooltip(
                         formatter = e_tooltip_item_formatter("percent")
                       ) |> 
                       e_flip_coords() |> 
                       e_title("Avance promedio por UR %", left = "center") |> 
                       e_color(color = "#8c5171")
                     
                     
                   })
                   
                   
                   
                   output$avanceeje1 <- renderEcharts4r({
                     readxl::read_excel("pruebas/tablas_dashboard.xlsx", sheet = "EJE1") |>
                       arrange(Serie) |> 
                       e_charts(Etiquetas) |> 
                       e_bar(Serie) |> 
                       e_flip_coords() |> 
                       e_legend(FALSE) |> 
                       e_color(color = "#eb8034") |> 
                       e_tooltip(
                         #trigger = "axis",
                         formatter = e_tooltip_item_formatter("percent")
                       ) |> 
                       e_title("Porcentaje de avance acumulado promedio de los responsables", left = "center")
                     
                   })
                   
                   output$donaeje1 <- renderEcharts4r({
                     readxl::read_excel("pruebas/tablas_dashboard.xlsx", sheet = "DONA1") |>
                       e_charts(Estado) |> 
                       e_color(color = c("#ebcc34", "#6aa358", "#d44848")) |> 
                       e_pie(Acciones, radius = c("50%", "70%")) |> 
                       e_tooltip(
                         trigger = "item",
                         #formatter = e_tooltip_item_formatter("percent")
                       ) |> 
                       e_legend(FALSE) |>
                       e_title("Porcentaje de estado de las acciones", left = "center")
                     
                     
                   })
                   
                   
                   output$avanceeje2 <- renderEcharts4r({
                     readxl::read_excel("pruebas/tablas_dashboard.xlsx", sheet = "EJE2") |>
                       arrange(Serie) |> 
                       e_charts(Etiquetas) |> 
                       e_bar(Serie) |> 
