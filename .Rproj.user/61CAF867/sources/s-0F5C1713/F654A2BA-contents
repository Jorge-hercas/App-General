                   ######## 
                   # Tablas de resumen por estrategia
                   
                   output$eobj1 <- function() {
                     obj1 %>% 
                       kable(digits = 2) %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(2:4, bold = F, color = "black", background = "rgb(243, 243, 243)") %>% 
                       column_spec(1:4, bold = T, color = "black", background = "rgb(243, 243, 243)") %>% 
                       row_spec(0, bold = T, color = "white", background = "rgb(18,160,154)") 
                     
                   }
                   
                   output$eobj2 <- function() {
                     obj2 %>% 
                       kable(digits = 2) %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(2:4, bold = F, color = "black", background = "white") %>% 
                       column_spec(1:4, bold = T, color = "black", background = "rgb(243, 243, 243)") %>% 
                       row_spec(0, bold = T, color = "white", background = "rgb(39,92,79)")
                   }
                   
                   
                   
                   
                   
                   output$eobj3 <- function() {
                     obj3 %>% 
                       kable(digits = 2) %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(2:4, bold = F, color = "black", background = "white") %>% 
                       column_spec(1:4, bold = T, color = "black", background = "rgb(243, 243, 243)") %>% 
                       row_spec(0, bold = T, color = "white", background = "rgb(151,28,130)")
                   }
                   
                   
                   output$eobj4 <- function() {
                     obj4 %>% 
                       kable(digits = 2) %>% 
                       kable_styling("bordered", full_width = F) %>% 
                       column_spec(2:4, bold = F, color = "black", background = "white") %>% 
                       column_spec(1:4, bold = T, color = "black", background = "rgb(243, 243, 243)") %>% 
                       row_spec(0, bold = T, color = "white", background = "rgb(241,143,0)")
                   }
                   
                   
                   ########### output para grafico de pie inicio
                   #### aquí se modifica el tamaño del pie
                   output$grafico <- renderggiraph(
                     girafe(
                       ggobj = p, 
                       width_svg = 15, height_svg = 10, bg = "#ffffff",
                       options = list(opts_sizing(rescale = FALSE),
                                      opts_hover(css = "fill:#ffffff;cursor:pointer;"),
                                      opts_hover_theme(css = "fill:red;cursor:pointer;"),
                                      opts_selection(css = "fill:#ffffff;cursor:pointer;", only_shiny = FALSE, selected = "J"),
                                      opts_tooltip(css = "background-color:white;padding:5px;border-radius:10px;border: white 1px solid;color:black;")
                       )
                     )
                     
                   )    
                   
