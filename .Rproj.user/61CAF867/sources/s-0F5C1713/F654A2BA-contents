#### Cargamos Bases para gráficos del inicio
# Gráfico de políticas públicas piechart

pie <- data.frame(
  stringsAsFactors = FALSE,
  individual = c("Política Industrial",
                 "Politica de Comercio Interior",
                 "Política de Comercio Exterior",
                 "Política de Atracción De Inversión Extranjera",
                 "Política de Desarrollo para la Competitividad de las MIPYMES",
                 "Política de Mejora Regulatoria",
                 "Política en Materia Minera",
                 "Política de Abasto",
                 "Política de Precios",
                 "Política de Protección al Consumidor",
                 "Política Nacional de Calidad",
                 "Política Nacional en Materia de Normalización, Estandarización, Evaluación de la Conformidad y Metrología",
                 "Política de Industrialización, Distribución y Consumo de los Productos Agrícolas, Ganaderos, Forestales, Minerales Y Pesqueros",
                 "Política Nacional de Fomento Economico",
                 "Política para Crear Y Apoyar Empresas que Asocien a Grupos de Escasos Recursos en Áreas Urbanas",
                 "Política de Fomento a las Compras de Proveedores Nacionales con los Sectores de Hidrocarburos y Electricidad"),
  group = c("A","A","A","A","B","B","B","B",
            "B","B","B","C","C","C","C","C"),
  'Objetivo Prioritario 1' = c(30L,3L,7L,3L,0L,3L,1L,2L,0L,1L,
                               7L,5L,1L,12L,0L,4L),
  'Objetivo Prioritario 2' = c(10L,0L,2L,0L,0L,6L,0L,0L,0L,11L,
                               7L,6L,0L,3L,1L,0L),
  'Objetivo Prioritario 3' = c(0L,0L,1L,2L,14L,2L,1L,0L,0L,0L,
                               1L,1L,0L,2L,0L,0L),
  'Objetivo Prioritario 4' = c(9L,0L,13L,8L,2L,5L,1L,1L,0L,0L,
                               0L,1L,2L,2L,0L,0L)
) %>% 
  setNames(c("individual", "group", "Objetivo Prioritario 1", "Objetivo Prioritario 2", "Objetivo Prioritario 3", "Objetivo Prioritario 4"))

pie = pie %>% 
  gather(key = "observation", value="value", -c(1,2)) 

empty_bar=2
nObsType=nlevels(as.factor(pie$observation))
to_add = data.frame( matrix(NA, empty_bar*nlevels(pie$group)*nObsType, ncol(pie)) )
colnames(to_add) = colnames(pie)
to_add$group=rep(levels(pie$group), each=empty_bar*nObsType )
pie=rbind(pie, to_add)
pie=pie %>% arrange(group, individual)
pie$id=rep( seq(1, nrow(pie)/nObsType) , each=nObsType)

# Get the name and the y position of each label
label_data= pie %>% group_by(id, individual) %>% summarize(tot=sum(value))
number_of_bar=nrow(label_data)
angle= 90 - 360 * (label_data$id-0.5) /number_of_bar     # I substract 0.5 because the letter must have the angle of the center of the bars. Not extreme right(1) or extreme left (0)
label_data$hjust<-ifelse( angle < -90, 1, 0)
label_data$angle<-ifelse(angle < -90, angle+180, angle)
rhg_cols <- c("#12A09A", "#1E5C4F", "#941B80", "#F19100")

# Make the plot
p <- ggplot(pie) +      
  geom_col_interactive(aes(x=as.factor(id), y=value, fill=observation,
                           tooltip = paste0(individual,": " , value)
  ), stat="identity", alpha=0.5) +
  scale_fill_manual_interactive(values = rhg_cols)+
  annotate_interactive("text", x = rep(max(pie$id),5), y = c(0, 50, 100, 150, 200), label = c("0", "50", "100", "150", "200") , 
                       color="grey", size=2 , angle=0, fontface="bold", hjust=1) +
  ylim(-25,max(label_data$tot, na.rm=T)) +
  theme_minimal() +
  theme(
    legend.position = "none",
    axis.text = element_blank(),
    axis.title = element_blank(),
    panel.grid = element_blank(),
    plot.margin = unit(rep(-1,4), "cm") 
  ) +
  coord_polar() + guides(fill=guide_legend(title=""))

