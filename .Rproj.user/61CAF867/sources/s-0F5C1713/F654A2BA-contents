#### datos gráficos avance
datos <- read_excel("Base_indicadores_SMI_abr2021_ampliada v1_06102021.xlsx", sheet = "Intermedios_validados_P1")
###

datos1 <- datos |> 
  select(`Histórico 2018`, `Histórico 2019`, `Avance 2020`)

names <- datos$`Número AP`

estrategicos_graf <- t(datos1) |> 
  as.data.frame() |> 
  setNames(names) |> 
  tibble::rownames_to_column("Periodo")

estrategicos_graf$Periodo <- recode(estrategicos_graf$Periodo, 
                                    "Histórico 2018" = "2018",
                                    "Histórico 2019" = "2019",
                                    "Avance 2020" = "Avance 2020"
)



estrategicos_graf |> 
  e_charts(Periodo) |> 
  e_bar(`1.1.4`, name = paste0("Estrategia ",names(estrategicos_graf[3]))) |> 
  e_color(color = "#2ca89c") |> 
  e_tooltip(trigger = "axis")



# Leyendo la base de datos
intermedios <- read_excel("www/intermedios.xlsx", sheet = "tablaR")


# filtrando para cada tabla
datos_fil <- intermedios |> 
  filter(Estatus == "Validado" ) |> 
  mutate(diferencia = as.numeric(`Avance 2020`)-as.numeric(`Histórico 2019`)  ) |> 
  mutate(
    logica = case_when(
      `Tendencia esperada` == "Ascendente" & diferencia >= 0 ~ "Cumple con tendencia",
      `Tendencia esperada` == "Descendente" & diferencia <= 0 ~ "Cumple con tendencia",
      `Tendencia esperada` == "Ascendente" & diferencia < 0 ~ "No cumple con tendencia",
      `Tendencia esperada` == "Descendente" & diferencia > 0 ~ "No cumple con tendencia",
    )
  ) 


completeFun <- function(data, desiredCols) {
  completeVec <- complete.cases(data[, desiredCols])
  return(data[completeVec, ])
}

datos_fil <- completeFun(datos_fil, "logica")

# Dividiendo para cada tabla

## objetivo 1

### Calculando porcentajes de cada estrategia

est_1.1 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "1.1") 

est_1.1 <- sum(est_1.1$logica == "Cumple con tendencia")/8

est_1.2 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "1.2") 

est_1.2 <- sum(est_1.2$logica == "Cumple con tendencia")/9

est_1.3 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "1.3") 

est_1.3 <- sum(est_1.3$logica == "Cumple con tendencia")/5

est_1.4 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "1.4") 

est_1.4 <- sum(est_1.4$logica == "Cumple con tendencia")/5

est_1.5 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "1.5") 

est_1.5 <- sum(est_1.5$logica == "Cumple con tendencia")/9

est_1.6 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "1.6") 

est_1.6 <- sum(est_1.6$logica == "Cumple con tendencia")/6



## objetivo 2

### Calculando porcentajes de cada estrategia

est_2.1 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "2.1") 

est_2.1 <- sum(est_2.1$logica == "Cumple con tendencia")/9

est_2.2 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "2.2") 

est_2.2 <- sum(est_2.2$logica == "Cumple con tendencia")/5

est_2.3 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "2.3") 

est_2.3 <- sum(est_2.3$logica == "Cumple con tendencia")/5

## objetivo 3

### Calculando porcentajes de cada estrategia

est_3.3 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "3.3") 

est_3.3 <- sum(est_3.3$logica == "Cumple con tendencia")/6

est_3.4 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "3.4") 

est_3.4 <- sum(est_3.4$logica == "Cumple con tendencia")/4


## objetivo 4

### Calculando porcentajes de cada estrategia

est_4.1 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "4.1") 

est_4.1 <- sum(est_4.1$logica == "Cumple con tendencia")/5

est_4.2 <- datos_fil |> 
  filter(substr(`Número AP`,1,3) == "4.2") 

est_4.2 <- sum(est_4.2$logica == "Cumple con tendencia")/10



# Creando las tablas

# objetivo 1

Objetivo_1 <- data.frame(
  Estrategia = c("1.1", "1.2", "1.3", "1.4", "1.5", "1.6", "1.7", "1.8"),
  `Nombre del indicador` = c("Porcentaje de indicadores de la estrategia 1.1 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.2 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.3 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.4 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.5 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.6 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.7 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior",
                             "Porcentaje de indicadores de la estrategia 1.7 
                             que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior"
  ),
  `Método de cálculo` = c("Número de indicadores de la estrategia 
                             1.1 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.1",
                          "Número de indicadores de la estrategia 
                             1.2 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.2",
                          "Número de indicadores de la estrategia 
                             1.3 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.3",
                          "Número de indicadores de la estrategia 
                             1.4 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.4",
                          "Número de indicadores de la estrategia 
                             1.5 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.5",
                          "Número de indicadores de la estrategia 
                             1.6 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.6",
                          "Número de indicadores de la estrategia 
                             1.7 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.7",
                          "Número de indicadores de la estrategia 
                             1.8 que cumplen con la tendencia esperada entre el periodo 
                             actual y el periodo inmediato anterior/ Total de indicadores 
                             de la estrategia 1.8"),
  Valor = round(c(est_1.1, est_1.2, est_1.3, est_1.4, est_1.5, est_1.6, 1.5,1.5), digits = 2))


